{
  "name": "vite-core",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pages": "tsc && vite build --base /fedimint-web-sdk/",
    "preview": "vite preview --base /fedimint-web-sdk/",
    "preview:pages": "vite preview --base /fedimint-web-sdk/",
