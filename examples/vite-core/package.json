    "clean:deep": "pnpm run clean && rm -rf node_modules"
  },
  "dependencies": {
    "@fedimint/core-web": "workspace:*",
    "react": "^18.3.1",
    "react-dom": ">=18.3.1"
  },
  "devDependencies": {
    "@types/react": ">=18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "buffer": "^6.0.3",
    "vite": "^5.3.2",
    "vite-plugin-wasm": "^3.3.0"
  }
}
