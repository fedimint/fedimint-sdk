{
  "name": "@fedimint/fedimint-web-sdk",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm run --r --filter \"./packages/**\" build",
    "build:wasm": "bash scripts/build_wasm.sh",
    "clean": "pnpm run --r --parallel clean && rm -rf packages/**/*.json.tmp",
    "clean:deep": "rm -rf node_modules && pnpm run --r --parallel clean:deep",
    "deps": "pnpx taze -r",
    "dev": "pnpm build && pnpm dev:vite",
    "dev:react": "pnpm --filter react dev",
    "dev:vite": "pnpm --filter vite-core dev",
    "dev:bare": "pnpm run --filter bare-js server",
    "dev:webpack": "pnpm --filter webpack-app build && pnpm --filter webpack-app start",
    "dev:next": "pnpm run --filter next-js dev",
    "docs:dev": "pnpm run --filter docs docs:dev",
    "docs:build": "pnpm run --filter docs docs:build",
    "docs:preview": "pnpm run --filter docs docs:preview",
    "typedoc": "pnpm run --r --filter \"./packages/**\" typedoc",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "lint:repo": "sherif",
    "preinstall": "pnpx only-allow pnpm",
    "prepare": "pnpm simple-git-hooks",
    "postinstall": "patch-package",
    "preview": "pnpm --filter vite-core preview",
    "reset": "pnpm clean && pnpm build && pnpm preview",
    "changeset": "changeset",
    "version": "changeset version",
    "typecheck": "pnpm run --r --parallel typecheck",
    "coverage": "vitest run --coverage",
    "test": "pnpm run test:setup pnpm run test:lib",
    "test:setup": "bash scripts/setup_test_shell.sh",
    "test:coverage": "vitest run --coverage",
    "test:lib": "vitest --project integration-tests",
    "test:create": "vitest --watch=false --project cli",
    "test:ui": "vitest --browser.headless=false --ui",
    "format": "prettier --write .",
    "watch": "pnpm run --r --parallel --filter \"./packages/**\" watch"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/bun": "^1.2.23",
    "@types/node": "^22.5.5",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "execa": "^9.6.0",
    "glob": "^10.4.5",
    "happy-dom": "^20.0.2",
    "patch-package": "^8.0.1",
    "playwright": "1.55.1",
    "prettier": "^3.6.2",
    "sherif": "^0.8.4",
    "simple-git-hooks": "^2.13.1",
    "typescript": "5.5.2",
    "unicorn-magic": "^0.3.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": "20.x"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint"
  }
}
