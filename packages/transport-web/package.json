{
  "name": "@fedimint/transport-web",
  "description": "Web worker transport for the Fedimint client",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/fedimint/fedimint-sdk.git",
    "directory": "packages/transport-web"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "./dist/worker.js"
  ],
  "scripts": {
    "build": "rollup --config",
