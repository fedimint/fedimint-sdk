{
  "name": "@fedimint/core-web",
  "description": "Library for building web apps with a fedimint client",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fedimint/fedimint-web-sdk.git",
    "directory": "packages/core-web"
  },
  "scripts": {
    "build": "rollup --config",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "clean:deep": "pnpm run clean && rm -rf node_modules",
    "typecheck": "tsc --noEmit",
    "typedoc": "typedoc"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "./dist/index.js"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@fedimint/transport-types": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.17.49",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
