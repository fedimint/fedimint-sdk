{
  "name": "@fedimint/core-web",
  "description": "Library for building web apps with a fedimint client",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/fedimint/fedimint-web-sdk.git",
    "directory": "packages/core-web"
  },
  "scripts": {
    "build": "rollup --config",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "clean:deep": "pnpm run clean && rm -rf node_modules",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src",
    "wasm"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "fedimint-client-wasm": "file:./pkg"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.8",
    "rollup": "^4.21.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.7.0",
    "typescript": "^5.2.2"
  }
}
